<div class="item<%= ' active' if commit_log_counter == 0 %>">
  <div class="carousel-caption-top carousel-caption">
    <h4><%= commit_log.name %></h4>
  </div>
  <div class="row-fluid">
    <ul class="thumbnails">
      <%= render(:partial => "commit", :collection => commit_log.commits) || render(:partial => 'success') %>
    </ul>
  </div>
</div>
